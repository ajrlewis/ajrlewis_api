{
    "env": {
        "PYTHONPATH": "api/"
    },
    "rewrites": [{
        "source": "/(.*)",
        "destination": "api/main.py"
    }],
    "builds": [{
        "src": "api/main.py",
        "use": "@vercel/python"
    }],
    "functions": {
        "api/main.py": {
            "maxDuration": 59
        }
    }
}
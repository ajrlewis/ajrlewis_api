{
    "env": {
        "PYTHONPATH": "src/"
    },
    "rewrites": [{
        "source": "/(.*)",
        "destination": "api/main.py"
    }],
    "functions": {
        "api/main.py": {
            "maxDuration": 60
        }
    },
    "buildCommand": "yum install wget -y && wget https://dl.google.com/linux/direct/google-chrome-stable_current_amd64.deb;"
}